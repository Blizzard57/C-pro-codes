#include<stdio.h>
int main()
{
    int l,d,n;

    scanf("%d",&d);
    for(l=0;l<d;l++)
    {
        int k,j,a,i,c=0,x,res_size=1,pdt,t,b;
        int res[1500];
        res[0]=1;

        scanf("%d",&n);
        for(x=1;x<=n;x++)
        {
            for(i=res_size-1;i>=0;i--)
            {
                pdt = res[i]*x+c;
                res[i] = pdt % 10;
                c = pdt / 10;
            }

            a=c;

            int cnt = 0;

            while(c)
            {
                c = c / 10;
                res_size++;
                cnt++;
            }

            if(a!=0)
            {
                for(j=res_size-1;j>=cnt;j--)
                    res[j]=res[j-cnt];

                for(k=cnt-1;k>=0;k--)
                {
                    b = a % 10;
                    res[k] = b;
                    a = a / 10;
                }
            }
        }
        for(t=0;t<res_size;t++)
            printf("%d",res[t]);
        printf("\n");
    }
    return 0;
}
